<div>
<%= form_for @week do |nw| %>
    <% if @week.errors.any? %>
        <% @week.errors.full_messages.each do |message| %>
            <%= message %>
        <% end %>
    <% end %>
    <%= nw.label :start_day %>
    <%= nw.date_field :start_day %>
    <%= nw.submit %>
<% end %>
</div>

<div>
    <% @weeks.each do |w| %>
        <div class="dates">
            <%= link_to "#{w.start_day}", week_path(w), :class => "date_link"  %>
        </div>
        <div id='week_info_<%= w.id%>'>
        </div>
        <%= link_to 'Go to Week', week_path(w) %>
    <% end %>
    <div id="ajax_links">
    </div>
</div>

